<div class="requests container">
  <h2 class="text-center">Requests</h2>
  <% @requests.each do |r| %>
      <div class="request">
        <% if not r.data.empty? %>
            <div class="data">
              <pre><%= r.data %></pre>
            </div>
            <div class="date"><%= r.created_at %></div>
            <hr/>
        <% end %>
      </div>
  <% end %>
</div>